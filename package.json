{
  "name": "contently-llm-analytics",
  "version": "0.0.1",
  "description": "Brand visibility analytics using LLMs",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/main.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:visibility": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/scripts/test-visibility-pipeline.ts",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d src/config/typeorm.config.ts",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/config/typeorm.config.ts",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d src/config/typeorm.config.ts",
    "test:integration": "jest --config ./jest.config.js \"src/**/*.integration.spec.ts\"",
    "test:smoke": "ts-node -r tsconfig-paths/register src/scripts/smoke-tests.ts",
    "test:health": "curl -f http://localhost:3000/health",
    "test:all": "npm run lint && npm test && npm run test:e2e",
    "test:all:cov": "npm run lint && npm run test:cov && npm run test:e2e",
    "fix:lint": "node scripts/fix-lint-issues.js",
    "prepare": "exit 0",
    "seed:brand-analytics": "node scripts/seed-brand-analytics.js",
    "seed": "ts-node src/cli.ts seed-database",
    "seed:watch": "ts-node src/cli.ts seed-database --watch",
    "check-schema": "ts-node src/scripts/check-schema.ts",
    "check-data": "ts-node src/scripts/check-data.ts",
    "verify-data": "ts-node src/scripts/verify-data.ts",
    "build:cli": "nest build && tsc -p tsconfig.json",
    "process-analytics": "npm run build:cli && node dist/cli.js process-analytics",
    "process-analytics:customer": "npm run build:cli && node dist/cli.js process-analytics --customer-id"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@fastify/cors": "^8.0.0",
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^5.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-fastify": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.0.0",
    "@nestjs/schedule": "^3.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/terminus": "^10.0.0",
    "@nestjs/websockets": "^10.0.0",
    "@sentry/node": "^9.2.0",
    "@sentry/tracing": "^7.120.3",
    "@types/bcrypt": "^5.0.2",
    "@types/cache-manager": "^4.0.6",
    "@types/joi": "^17.2.3",
    "@types/passport-google-oauth20": "^2.0.16",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^4.1.0",
    "docker-compose": "^1.1.1",
    "graphql": "^16.10.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-ws": "^5.14.0",
    "joi": "^17.13.3",
    "nest-commander": "^3.16.1",
    "openai": "^4.86.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.13.3",
    "prom-client": "^15.1.3",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "supabase-cli": "^0.0.21",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@eslint/js": "^9.21.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@supabase/supabase-js": "^2.49.1",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.11.7",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.23",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "better-sqlite3": "^11.8.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.21",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0",
    "yargs": "^16.2.0"
  }
}
